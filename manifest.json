{
    "manifest_version": 2,
    "name": "Re-Focus",
    "description": "A Productivity Extension which helps you re-focus everytime you open a new tab.",
    "version": "1.0",

    "chrome_url_overrides" : {
        "newtab": "index.html"
    },

    "permissions":[
        "activeTab"
    ],

    "browser_action":{
        "default_popup" : "index.html",
        "default_title": "Re-Focus"   
    }
}